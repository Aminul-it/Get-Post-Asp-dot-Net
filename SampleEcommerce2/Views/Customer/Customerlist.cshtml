@model List<CustomerDetailsVM>
@foreach(CustomerDetailsVM customers in Model){



    <div class="card">
        <h5 class="card-header"> Customer Name @customers.Name</h5>
        <div class="card-body">
            <p>Customer Id @customers.Id</p>
            <p> Customer Name @customers.Name</p>
            <p>Customer PhoneNo @customers.PhoneNo</p>
            <p>Customer Address @customers.Address</p>
        </div>
    </div>
    
}